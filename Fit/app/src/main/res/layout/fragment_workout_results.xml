<com.kinetic.fit.ui.widget.FitLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kinetic.fit.ui.workout.WorkoutResultsFragment">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.5"
        android:textSize="31dp"
        android:gravity="center"
        android:text="Workout Summary" />


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="7.5">
    </FrameLayout>

    <Button
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/button_workout_results_done"
        android:text="Done"
        android:layout_weight="1"/>



</com.kinetic.fit.ui.widget.FitLinearLayout>
